{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "semver init",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/main.go",
        "args": ["init"]
      },
      {
        "name": "semver up major",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/main.go",
        "args": ["up", "major"]
      },
      {
        "name": "semver reset",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/main.go",
        "args": ["reset"]
      },
      {
        "name": "semver release",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/main.go",
        "args": ["release", "-d"]
      }
    ]
  }
  